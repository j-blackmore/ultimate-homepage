{
  "singleQuote": true,
  "arrowParens": "always",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "jsxBracketSameLine": false,
  "useTabs": false,
  "printWidth": 100,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": false,
  "tabWidth": 2,
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": [".*rc"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
